  "lint-staged": {
    "src/**/*.{css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,json}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
